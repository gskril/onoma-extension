{
	"manifest_version": 3,
	"name": "Onoma",
	"description": "Name EVM addresses on the fly",
	"version": "0.0.1",
	"icons": {
		"16": "icons/icon_16.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png",
		"256": "icons/icon_256.png"
	},
  "permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["bundle.js"]
		}
	]
}